{
  "name": "private-vehicle-insurance",
  "version": "2.0.0",
  "description": "Private Vehicle Insurance Platform with FHE",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.hardhat.json hardhat test",
    "test:coverage": "cross-env TS_NODE_PROJECT=./tsconfig.hardhat.json hardhat coverage",
    "deploy:local": "hardhat deploy --network localhost",
    "deploy:sepolia": "hardhat deploy --network sepolia",
    "node": "hardhat node",
    "typechain": "hardhat typechain",
    "size": "hardhat size-contracts",
    "verify": "hardhat verify",
    "clean": "hardhat clean",
    "dev": "next dev -p 1362",
    "build": "next build",
    "start": "next start -p 1362",
    "lint": "next lint",
    "lint:sol": "solhint \"contracts/**/*.sol\"",
    "lint:fix": "solhint \"contracts/**/*.sol\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,sol}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,sol}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "fhe",
    "insurance",
    "privacy",
    "zama",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.1.0",
    "@fhevm/mock-utils": "^0.1.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.13",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.13",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "autoprefixer": "^10.4.0",
    "chai": "^4.3.10",
    "cross-env": "^10.1.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.22.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-deploy-ethers": "^0.4.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-solidity": "^2.1.0",
    "solhint": "^6.0.1",
    "solidity-coverage": "^0.8.5",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.0",
    "typechain": "^8.3.0",
    "typescript": "^5.5.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.8.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@rainbow-me/rainbowkit": "^2.1.0",
    "@tanstack/react-query": "^5.51.0",
    "@zama-fhe/oracle-solidity": "^0.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.0.3",
    "fhevmjs": "^0.5.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.4.0",
    "viem": "^2.17.0",
    "wagmi": "^2.12.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ],
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "ES2022",
      "esModuleInterop": true
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.sol": [
      "prettier --write",
      "solhint"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
